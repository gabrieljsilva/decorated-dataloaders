{
	"name": "decorated-dataloaders",
	"description": "A module that helps you to create graphql dataloaders with decorators in Nest.js",
	"version": "1.0.2",
	"main": "dist/index.js",
	"license": "MIT",
	"files": [
		"dist"
	],
	"types": "dist/index.d.ts",
	"author": {
		"email": "contato@gabrieljs.dev",
		"name": "Gabriel de Jesus Silva",
		"url": "https://gabrieljs.dev"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gabrieljsilva/decorated-factory"
	},
	"scripts": {
		"test": "vitest --run",
		"pretest:coverage": "rimraf coverage",
		"test:coverage": "vitest --coverage --run",
		"prebuild": "rimraf dist",
		"build": "tsc --build tsconfig.build.json",
		"postbuild": "yarn local-pack",
		"prelocal-pack": "rimraf decorated-dataloaders-*.tgz && npm version patch -f",
		"local-pack": "npm pack"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
