		"dataloader": "^2.2.2",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.0.1",
		"rxjs": "^7.8.1",
		"typescript": "^5.5.3",
		"vitest": "^2.0.3"
	},
	"peerDependencies": {
		"@nestjs/common": "^10.0.0",
		"@nestjs/core": "^10.0.0",
		"reflect-metadata": "^0.2.2",
